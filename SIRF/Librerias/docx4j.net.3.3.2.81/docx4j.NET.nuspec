      <dependency id="IKVM" version="8.1.5717.0" />
      <dependency id="slf4j-api" version="1.7.5.4" />
      <dependency id="Common.Logging" version="2.1.2" />
      <dependency id="slf4j-NetCommonLogging" version="1.7.5.4" />
    </dependencies>
  </metadata>
