                                    <asp:boundfield headertext="Tipo de solicitud" datafield="TipoSolicitud.DescripcionTipoSolicitud" />
                                    <asp:boundfield headertext=" @MsnVentanilla.HEADER_GRID_UNIDAD_ADMINISTRATIVA " datafield=" unidadadministrativa.unidadadministrativa" />
                                    <asp:boundfield headertext="Prioridad" datafield="Prioridad.DescripcionPrioridad" visible="false" />
                                    <asp:boundfield headertext="EstatusID" datafield="Estatus.EstatusID" visible="false" />
                                    <asp:boundfield headertext="Estatus" datafield="Estatus.DescripcionEstatus" />
