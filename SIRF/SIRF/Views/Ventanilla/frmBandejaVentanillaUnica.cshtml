                                                                Visible='<%# (bool.Parse(Eval("Rechazada").ToString())) '>
                                                    <span class="glyphicon glyphicon-eye-close"></span>
                                                </asp:LinkButton>
                                            </div>
                                        </itemtemplate>
                                    </asp:templatefield>
                                </columns>
                                <headerstyle backcolor="#335A8E" forecolor="White" horizontalalign="Center" />
                                <rowstyle backcolor="White" />
                                <emptydatarowstyle forecolor="#335a8e" cssclass="table table-bordered" />
                                <emptydatatemplate>
                                    @Mensajes.infNoExisteInformacionGrids
                                </emptydatatemplate>
                            </asp:GridView>-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
