                                    <asp:boundfield headertext="Observaciones" datafield="Observaciones" visible="false" />
                                    <asp:boundfield headertext="Oficio" datafield="OficioSolicitud" />
                                    <asp:boundfield headertext="Rechazada" datafield="Rechazada" visible="false" />
                                    <asp:boundfield headertext="Tipo Firma" datafield="Descripcion" />
